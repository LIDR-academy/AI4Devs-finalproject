NODE_ENV=development
PORT=3001

# Base de Datos MySQL
DATABASE_URL=mysql://zonmatch_user:zonmatch_pass@localhost:3306/zonmatch
DB_HOST=localhost
DB_PORT=3306
DB_NAME=zonmatch
DB_USER=zonmatch_user
DB_PASSWORD=zonmatch_pass

# Redis Cache
REDIS_URL=redis://localhost:6379
