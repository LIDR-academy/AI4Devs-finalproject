    "extends": "./tsconfig.json",
    "compilerOptions": {
        "outDir": "./out-tsc/spec",
        "types": ["jasmine"]
    },
    "include": ["src/**/*.spec.ts", "src/**/*.d.ts"],
    "exclude": [
        "src/app/shared/**/*.spec.ts",
        "src/app/modules/admin/**/*.spec.ts",
        "src/app/modules/landing/**/*.spec.ts"
    ]
}
